//This is Problem

for (var i = 0; i <= 5; i++){
    setTimeout(function () {
        console.log(i);
    }, 1000 * i)
}

// Solution

for (var i = 1; i <= 5; i++) {
    ;(function (n) {
        setTimeout(function () {
            console.log(n)
        }, 1000 * n)
    })(i)
}

// Prototype হল function এর একটি প্রপার্টি জেতা একটি অবজেক্ট কে পয়েন্ট করে। 
